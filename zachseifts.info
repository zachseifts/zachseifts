name = zachseifts
description = A bootstrap based theme for zach.seifts.us
core = 7.x
version = 7.x-1.x
base theme = bootstrap

; Regions
regions[help]           = 'Help'
regions[content]        = 'Content'
regions[footer]         = 'Footer'

; Stylesheets
stylesheets[all][] = css/screen.css

; Grab bootstrap from the cdn
settings[toggle_name] = 0
settings[toggle_search] = 0
settings[cdn_bootstrap] = 1
settings[cdn_bootstrap_version] = 2.3.2
